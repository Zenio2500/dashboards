<div class="row">
  <div class="col-xs-12">
    <h1>Criar Novo Dashboard</h1>
  </div>

  <div class="col-md-6 col-md-offset-3 col-xs-12">
  
  <%= form_with model: [current_account, @dashboard] do |form| %>

    <%= render "form", f:form %>

    <%= form.submit "Criar", class: "btn btn-primary" %>
  
  <% end %>  
  </div>
</div>

<p></p>

<div id="ld">
    <%= link_to "Lista de Dashboards", account_dashboards_path(current_account), class: 'btn btn-primary btn-sm' %>
</div>